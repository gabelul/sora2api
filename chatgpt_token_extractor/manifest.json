{
    "manifest_version": 3,
    "name": "ChatGPT Token Extractor",
    "version": "1.0",
    "description": "Extracts session cookie and access token from ChatGPT",
    "permissions": [
        "cookies",
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://chatgpt.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}